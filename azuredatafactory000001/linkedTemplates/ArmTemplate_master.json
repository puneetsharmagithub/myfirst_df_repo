{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azuredatafactory000001"
		},
		"ls_ablob_source1989_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_source1989'"
		},
		"ls_adls_destination_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_destination'"
		},
		"ls_adls_destination_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destination.dfs.core.windows.net"
		},
		"ls_gothub_download_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/cloudboxacademy/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredatafactory000001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_source1989_connectionString": {
						"value": "[parameters('ls_ablob_source1989_connectionString')]"
					},
					"ls_adls_destination_accountKey": {
						"value": "[parameters('ls_adls_destination_accountKey')]"
					},
					"ls_adls_destination_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_destination_properties_typeProperties_url')]"
					},
					"ls_gothub_download_properties_typeProperties_url": {
						"value": "[parameters('ls_gothub_download_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}